<%= simple_form_for @event  do |f| %>
<%= f.input :title %>
<%= f.input :cover %>
<%= f.input :where %>
<%= f.input :cords %>
<%= f.input :start, as: :datetime, html5: true %>
<%= f.input :end, as: :datetime, html5: true %>
<%= f.input :description %>
<%= f.input :tags %>
<%= f.button :submit, class:"btn-primary btn-block" %>
<% end %>